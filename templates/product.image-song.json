    "product_faq": {
      "type": "product_faq",
      "blocks": {
        "467c2187-40bf-47df-aa10-97ff398d79eb": {
          "type": "column",
          "settings": {
            "title": "What is the canvas made of?",
            "text": "Each canvas is made with choice poly-cotton canvas. Includes Lifetime UV protection to stay vibrant. Solid wood backing."
          }
        },
        "c86c223c-d320-4367-957c-1da15e7f5c9b": {
          "type": "column",
          "settings": {
            "title": "Do you have my song?",
            "text": "Yes! We can do any song. Just let us know what song you want and we can do the rest."
          }
        },
        "fd5bfc89-bec4-43a3-97b5-e2fa5f87f7f3": {
          "type": "column",
          "settings": {
            "title": "How do i order?",
            "text": "Select the size and quantity of canvas and any customizations and we will do the rest! We will send a preview within 3-4 business days to get your approval before production\/shipment."
          }
        },
        "8168bc97-e58d-4a2d-8fd9-c9fa8f34aa44": {
          "type": "column",
          "settings": {
            "title": "What is the best size for me?",
            "text": "Our most popular size is the 24x48''. We do recommend measuring the space where you want the canvas to be hung."
          }
        },
        "96575bac-0453-49a7-9cb7-f383de220d85": {
          "type": "column",
          "settings": {
            "title": "What is the turnaound time?",
            "text": "Short answer with standard shipping around 2 weeks. Priority shipping 1 week. The sooner the proof is approve the faster we can get it out."
          }
        },
        "5866d0af-7056-4b89-9f63-32f69bc011a5": {
          "type": "column",
          "settings": {
            "title": "Does the canvas arrive ready to hang?",
            "text": "Yes, it arrives ready to hang with a wooden backing. All you need to do is decide where to hang your new canvas."
          }
        }
      },
